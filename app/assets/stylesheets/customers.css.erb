.UsersNew-title {
  margin-bottom: 16px;
  font-size: 30px;
  font-family: 'Bebas Neue', cursive;
  color: rgba(217, 30, 24, 1);
  background-color: rgba(0,0,0,0.7);
  padding: 5px;
  text-align: center;
  border-radius: 12px;
  margin-top: 20px;
}

.FormSection {
  margin-bottom: 30px;
  font-family: 'Cinzel', serif;
  background-color: rgba(0,0,0,0.7);
  padding: 10px;
  border-radius: 12px;
}

.Form-Input {
  font-size: 20px;
  padding: 8px;
  margin-left: 10px;
  border-radius: 12px;
}

.FormInput-label {
  font-size: 20px;
  color: #555555;
  margin-bottom: 8px;
  color:  white;
  font-weight: 700;
}

.FormSubmit {
  font-size: 20px;
  padding: 10px;
  background: rgba(241, 130, 141,1);
  font-weight: bolder;
  border: 1px solid transparent;
  border-radius: 4px;
  color: black;
}
